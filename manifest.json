{
  "manifest_version": 3,
  "name": "Strava kudo em all",
  "version": "1.0",
  "description": "Because everyone deserves a little recognition",
  "license": "MIT",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
} 